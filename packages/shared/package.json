{
  "name": "pyrajs-shared",
  "version": "0.2.1",
  "description": "Shared utilities and types for Pyra.js",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest run --config vitest.config.ts",
    "build": "tsup src/index.ts --dts --format esm --out-dir dist",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "pyra",
    "utilities",
    "shared",
    "types"
  ],
  "author": "Nathaniel Paz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simpleboi/Pyra",
    "directory": "packages/shared"
  },
  "homepage": "https://github.com/Simpleboi/Pyra#readme",
  "bugs": {
    "url": "https://github.com/Simpleboi/Pyra/issues"
  },
  "dependencies": {
    "picocolors": "^1.1.1"
  }
}
