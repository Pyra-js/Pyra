{
  "name": "create-pyra",
  "version": "0.16.1",
  "description": "Scaffold a new Pyra.js project",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "create-pyra": "bin/create-pyra.js"
  },
  "files": [
    "bin",
    "dist",
    "template-react-ts",
    "template-react-js",
    "template-react-spa-ts",
    "template-react-spa-js",
    "template-vanilla-ts",
    "template-vanilla-js",
    "template-preact-ts",
    "template-preact-js",
    "template-preact-spa-ts",
    "template-preact-spa-js"
  ],
  "scripts": {
    "build": "tsup",
    "dev:link": "pnpm build && npm link",
    "dev:unlink": "npm unlink -g create-pyra",
    "prepublishOnly": "pnpm build",
    "testing-wd": "cd ../../ && pnpm run dev:link && cd packages/create-pyra && node dist/index.js"
  },
  "keywords": [
    "pyra",
    "create",
    "scaffold",
    "react",
    "fullstack",
    "ssr",
    "framework"
  ],
  "author": {
    "name": "Nathaniel Paz",
    "email": "natejsx@gmail.com",
    "url": "https://natejsx.dev/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Natejsx/Pyra.git",
    "directory": "packages/create-pyra"
  },
  "homepage": "https://github.com/Natejsx/Pyra#readme",
  "bugs": {
    "url": "https://github.com/Natejsx/Pyra/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.9.1",
    "picocolors": "^1.1.1"
  }
}
